<h1> portfolios posts</h1>
<%= @portfolios.inspect %>
</br>
<% @portfolios. each do|portfolio_item| %>
    <p><%= portfolio_item.title %> </p>
    <p><%= portfolio_item.subtitle %></p>
    <p><%= portfolio_item.body %></p>
    <p><%= portfolio_item.thumb_image %></p>
      <p><%= image_tag portfolio_item.thumb_image %></p>

<%end%>
